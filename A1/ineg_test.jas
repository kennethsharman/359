.main		// main program

start:
	BIPUSH	0x01	// Push 1 to stack
	INEG		// Negate value on top of stack
			// Check TOS value in Mic1MMV: 0x01 -> 0xff
	
	BIPUSH 0xc7	// Push 199 to stack
	INEG		// Negate
			// Check TOS value in Mic1MM: 0xc7 -> 0x39

	HALT		// end program

.end-main
